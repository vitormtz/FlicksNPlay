	<queryString>
		<![CDATA[SELECT u.id_usuario, u.nome, COALESCE(f.nome, j.nome) AS filme_jogo, COALESCE(f.tipo, j.tipo) AS tipo, l.vl_total, l.dt_inicio
FROM usuarios u INNER JOIN locacoes l ON u.id_usuario = l.id_usuario
